{
  "name": "nextdoc",
  "description": "Fetch Markdown files and render them as full pages.",
  "keywords": [
    "documentation",
    "markdown"
  ],
  "author": "Luiz Américo Pereira Câmara",
  "version": "0.9.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/blikblum/nextdoc.git"
  },
  "scripts": {
    "reference": "dox -r < nextdoc.js | node support/dox2md.js --default-level 3 > Reference.md",
    "test": "mocha",
    "theme-white": "stylus -U -u nib < theme-white/style.styl > theme-white/style.css"
  },
  "main": "nextdoc.js",
  "devDependencies": {
    "chai": "4.2.0",
    "commander": "4.0.0",
    "dox": "0.9.0",
    "jsdom": "16.5.0",
    "mocha": "6.2.2",
    "nib": "1.1.2",
    "stylus": "0.54.7",
    "uglify-js": "3.6.7"
  }
}
